@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<ComicRackWebViewer.List>
{
  d: {
    __count: @Model.Comics.Count()
    ,results: [
        @foreach (var item in Model.Comics)
        {
          string series = item.Series.ToString().Replace("\"", "\\\"");
          string number = item.Number.ToString().Replace("\"", "\\\"");
          string title = item.Title.ToString().Replace("\"", "\\\"");
          string volume = item.Volume.ToString().Replace("\"", "\\\"");

          var s = "{ \"Id\": \"" + @item.Id + "\", \"Series\": \"" + series + "\", \"Volume\": \"" + volume + "\", \"Number\": \"" + number + "\", \"Title\": \"" + title + "\", \"Month\": \"" + item.Month + "\", \"Year\": \"" + item.Year + "\" },";
          Write(s);
                      
        }
    ]
  }
}
